{
  "name": "food-styles-server",
  "version": "1.0.0",
  "description": "Food Styles GraphQL backend API",
  "main": "build/app/index.js",
  "scripts": {
    "start": "nodemon ./dist/index.js",
    "build": "webpack",
    "build-start": "yarn schema:build && yarn build && yarn start",
    "lint": "eslint './**/*.ts'",
    "schema:build": "graphql-schema-utilities -d -s './src/graphQL/schema/{queries,mutations}/*.graphql' -o ./src/graphQL/schema/schema.graphql"
  },
  "repository": "git+https://github.com/robecor1/food-styles-server.git",
  "author": "Corolea Robert Cozmin",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/robecor1/food-styles-server/issues"
  },
  "homepage": "https://github.com/robecor1/food-styles-server#readme",
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@typescript-eslint/parser": "^5.57.0",
    "async_hooks": "^1.0.0",
    "eslint": "^8.37.0",
    "express": "^4.18.2",
    "express-graphql": "^0.12.0",
    "graphql": "^16.6.0",
    "graphql-schema-utilities": "^1.1.6",
    "graphql-tag": "^2.12.6",
    "net": "^1.0.2",
    "path": "^0.12.7",
    "prettier": "^2.8.7",
    "sequelize": "^6.30.0",
    "sqlite3": "^5.1.6",
    "ts-loader": "^9.4.2",
    "typescript": "^5.0.3"
  },
  "devDependencies": {
    "webpack": "^5.77.0",
    "webpack-cli": "^5.0.1",
    "webpack-graphql-loader": "^1.0.2"
  }
}
